{
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "timezone": "America/New_York",
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchPackagePatterns": [
        ".*eslint.*",
        ".*rollup.*",
        "@types.*",
        "esbuild-register",
        "esbuild",
        "mocha",
        "tslib",
        "typescript"
      ],
      "automerge": true,
      "automergeType": "branch"
    }
  ]
}
