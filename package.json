{
  "name": "slip-sockets",
  "version": "0.0.0-development",
  "description": "A serverless GRIP/Pushpin like WebSocket server for AWS API Gateway",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index-esm.mjs"
  },
  "packageManager": "pnpm@8.7.0",
  "engines": {
    "node": ">=18"
  },
  "repository": "git@github.com:reconbot/SLIP-Sockets.git",
  "homepage": "https://github.com/reconbot/SLIP-Sockets",
  "scripts": {
    "test": "node -r esbuild-register --test lib/*-test.ts",
    "lint": "tsc && eslint lib/**/*.ts",
    "format": "eslint lib/**/*.ts --fix",
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c && node -r esbuild-register bundle-types",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "cdk": "cdk",
    "deploy:production": "cdk deploy '*production' --outputs-file ./cdk-outputs.json",
    "deploy:staging": "cdk deploy '*staging' --outputs-file ./cdk-outputs.json"
  },
  "keywords": [
    "websockets",
    "lambda",
    "api-gateway",
    "cdk"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.91.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.91.0-alpha.0",
    "@aws-sdk/client-apigatewaymanagementapi": "3.388.0",
    "@aws-sdk/client-dynamodb": "3.388.0",
    "@aws-sdk/lib-dynamodb": "3.388.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-typescript": "11.1.3",
    "@types/aws-lambda": "8.10.119",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.5.6",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "aws-cdk-lib": "2.93.0",
    "aws-cdk": "2.93.0",
    "aws-lambda": "1.0.7",
    "constructs": "10.2.69",
    "dts-buddy": "0.1.14",
    "esbuild-register": "3.4.2",
    "esbuild": "0.19.2",
    "eslint": "8.48.0",
    "rollup": "3.28.1",
    "semantic-release": "21.1.1",
    "source-map-support": "0.5.21",
    "streaming-iterables": "8.0.0",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "wscat": "5.2.0"
  },
  "dependencies": {
    "jsonwebtoken": "9.0.1"
  }
}
